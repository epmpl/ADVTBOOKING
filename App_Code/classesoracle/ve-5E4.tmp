                OracleParameter prm3 = new OracleParameter("docno", OracleType.VarChar, 50);
                prm3.Direction = ParameterDirection.Input;
                prm3.Value = docno;
                objOraclecommand.Parameters.Add(prm3);

                OracleParameter prm4 = new OracleParameter("agencycode", OracleType.VarChar, 50);
                prm4.Direction = ParameterDirection.Input;
                prm4.Value = agencycode;
                objOraclecommand.Parameters.Add(prm4);

                OracleParameter prm5 = new OracleParameter("rono", OracleType.VarChar, 50);
                prm5.Direction = ParameterDirection.Input;
                prm5.Value = rono;
                objOraclecommand.Parameters.Add(prm5);

                OracleParameter prm6 = new OracleParameter("keyno", OracleType.VarChar, 50);
                prm6.Direction = ParameterDirection.Input;
                prm6.Value =keyno;
                objOraclecommand.Parameters.Add(prm6);

                OracleParameter prm7 = new OracleParameter("client", OracleType.VarChar, 50);
                prm7.Direction = ParameterDirection.Input;
                prm7.Value = client;
                objOraclecommand.Parameters.Add(prm7);

                OracleParameter prm8 = new OracleParameter("booking", OracleType.VarChar, 50);
                prm8.Direction = ParameterDirection.Input;
                prm8.Value = booking;
                objOraclecommand.Parameters.Add(prm8);

                objOraclecommand.Parameters.Add("P_ACCOUNTS", OracleType.Cursor);
                objOraclecommand.Parameters["P_ACCOUNTS"].Direction = ParameterDirection.Output;

